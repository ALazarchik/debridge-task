{
    "compilerOptions": {
        "esModuleInterop": true,
        "strict": true,
        "strictNullChecks": true,
        "skipLibCheck": true,
        "noEmit": true,
        "baseUrl": ".",
        "paths": {
            "@e2e-support/*": [
                "support/*"
            ],
            "@e2e-testConfig": [
                "test.config.ts"
            ],
            "@e2e-components/*": [
                "support/ui/components/*"
            ],
            "@e2e-modals/*": [
                "support/ui/modals/*"
            ],
            "@e2e-pages/*": [
                "support/ui/pages/*"
            ]
        }
    },
    "include": [
        "./**/*"
    ],
    "exclude": [
        "playwright-report"
    ]
}